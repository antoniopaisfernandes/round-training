{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "cypress:run": "node_modules/.bin/cypress run --project ./tests/Acceptance",
        "cypress:open": "node_modules/.bin/cypress open --project ./tests/Acceptance",
        "cypress:prep": "node tests/Acceptance/setup.js",
        "cypress:teardown": "node tests/Acceptance/teardown.js",
        "cypress:test": "npm run cypress:prep && (((concurrently \"npx wait-on tcp:8000 && npm run cypress:run\" --kill-others \"php artisan serve\" --success first) && npm run cypress:teardown) || (npm run cypress:teardown && bash -c 'false'))"
    },
    "dependencies": {
        "@mdi/font": "^5.0.45",
        "axios": "^0.19",
        "date-fns": "^2.13.0",
        "file-saver": "^2.0.2",
        "js-file-download": "^0.4.12",
        "lodash-es": "^4.17.13",
        "tailwindcss": "^1.4.6",
        "vue": "^2.5.17",
        "vue-api-query": "^1.5.1",
        "vuetify": "^2.2.27"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "browser-sync": "^2.26.7",
        "browser-sync-webpack-plugin": "^2.2.2",
        "concurrently": "^5.2.0",
        "cross-env": "^7.0",
        "cypress": "3.4.1",
        "laravel-mix": "^5.0.4",
        "laravel-mix-purgecss": "^5.0.0",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.10",
        "vuetify-loader": "^1.4.3",
        "wait-on": "^4.0.2"
    }
}
